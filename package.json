{
  "name": "Hpmerchant",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && watchman watch-del-all",
    "scan": "sonar-scanner",
    "new:screen": "npx ts-node ./scripts/createScreen.js",
    "new:page": "npx ts-node ./scripts/createPage.js",
    "new:component": "npx ts-node ./scripts/createComponent.js",
    "new:redux": "npx ts-node ./scripts/createRedux.js",
    "new:api": "npx ts-node ./scripts/createApi.js",
    "scroll:fix": "npx ts-node ./scripts/react-scrollview-fix.js",
    "____android____": " ================= Script for Android ================= ",
    "run:android": "npx react-native run-android",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "____ios____": " ================= Script for iOS ================= ",
    "run:ios-production": "npx react-native run-ios --scheme='Hpmerchant_Production' --simulator='iPad Pro (9.7-inch)'",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "pod:update": "cd ios && pod install --repo-update && cd ..",
    "pod": "npx pod-install",
    "postinstall": "node scripts/react-scrollview-fix.js",
    "pod:clean": "cd ios && pod deintegrate && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && rm -rf Podfile.lock",
    "push:ios-production": "appcenter codepush release-react -a HarmonyPayment/HpMerchant-iOS -d Production -t 1.1 --description update",
    "push:ios-staging": "appcenter codepush release-react -a HarmonyPayment/HpMerchant-iOS -d Staging -t 1.2.5 --description retailer",
    "push:ios-development": "appcenter codepush release-react -a HarmonyPayment/HpMerchant-iOS -d Development -t 1.1 --description update",
    "run:ios-staging": "npx react-native run-ios --scheme='Hpmerchant_Staging' --simulator='iPad Pro (9.7-inch)'",
    "run:ios-development": "npx react-native run-ios --scheme='Hpmerchant_Dev' --simulator='iPad Pro (9.7-inch)'",
    "push-clean:ios-staging": "appcenter codepush deployment clear -a HarmonyPayment/HpMerchant-iOS Staging",
    "push-clean:ios-development": "appcenter codepush deployment clear -a HarmonyPayment/HpMerchant-iOS Development",
    "____device____": " ================= Script for Device ================= ",
    "build:ios": "react-native bundle --entry-file='./index.js' --bundle-output='./ios/Hpmerchant/main.jsbundle' --dev=false --platform='ios'",
    "android-dev": "adb reverse tcp:8081 tcp:8081",
    "reload-android": "adb shell input text RR",
    "ios:dev": "react-native run-ios --scheme='Hpmerchant_Dev' --simulator='iPad Pro (12.9-inch) (4th generation)' ",
    "ios:staging": "react-native run-ios --scheme='Hpmerchant_Staging' --simulator='iPad Pro (12.9-inch) (4th generation)' ",
    "ios:production": "react-native run-ios --scheme='Hpmerchant_Production' --simulator='iPad Pro (12.9-inch) (4th generation)' ",
    "open:iPad": "open -a Simulator --args -CurrentDeviceUDID  34E1A130-28A3-4871-972B-A445C037BC22",
    "zeplin-connect": "zeplin connect",
    "zeplin-connect-dev-mode": "zeplin connect --dev"
  },
